{
  "name": "react-shopify-theme",
  "version": "0.1.2",
  "description": "React components for Shopify themes",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/react-shopify-theme.js && webpack -p src/index.js dist/react-shopify-theme.min.js",
    "clean": "rimraf lib",
    "prepublish": "npm run test && npm run clean && npm run build && npm run build:umd",
    "test": "mocha --compilers js:babel-register --inline-diffs test/**"
  },
  "author": "Steven Nguyen",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=15.0.0",
    "react-dom": ">=15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.7.2",
    "classname": "0.0.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "expect": "^1.18.0",
    "jsdom": "^9.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "react": "^15.0.1",
    "rimraf": "^2.5.2",
    "typescript": "^1.8.9",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "lodash": "^4.12.0",
    "preamble-utils": "^3.0.1"
  }
}
